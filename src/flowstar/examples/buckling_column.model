continuous reachability
{
 state var x,y
	
 setting
 {
  fixed steps 0.01
  time 10
  remainder estimation 1e-8
  QR precondition
  gnuplot octagon x,y
  adaptive orders { min 4, max 8}
  cutoff 1e-12
  precision 53
  output buckling_column
  print on
 }
	
 poly ode 2
 {
  x' = y
  y' = 2 * x - x^3 - 0.2 * y + 0.1
 }
 
 init
 {
  x in [-0.5 , -0.4]
  y in [-0.5 , -0.4]
 }
}
